  cin>>celsius;
  
  double Farenheyt_celsiusi (int );
  int elq=Farenheyt_celsiusi(celsius);
  cout<<endl<<celsius<<" Celsius@ "<<elq<<" farenheyti e havasar.";// endl mi tox shift arec xi? karoxa harc@ cin-i mej a
  
  
