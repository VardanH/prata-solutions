// Example program
#include <iostream>

using namespace std;

int main()
{  
   cout<<"Mutqagri lusatarin :";
   double lusatari;
  cin>>lusatari;
  
  double Lusatarin_Tmiavaorov ( double);
  double elq=Lusatarin_Tmiavaorov (lusatari);
  
  cout<<lusatari<<" lusatarin"<<" = "<<elq<<" Tiezerakan miavori.";
  
  
   return 0;
}
 double Lusatarin_Tmiavaorov (double a){
     
     
    double elq=a*63240;
     
     
     return  elq;        
     }
